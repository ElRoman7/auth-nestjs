{
    "material-icon-theme.files.associations": {
        "*.module.ts": "nest-module",
        "*.controller.ts": "nest-controller",
        "*.service.ts": "nest-service",
        "*.middleware.ts": "nest-middleware",
        "*.filter.ts": "nest-filter",
        "*.pipe.ts": "nest-pipe",
        "*.gateway.ts": "nest-gateway",
        "*.decorator.ts": "nest-decorator",
        "*.guard.ts": "nest-guard",
        "*.resolver.ts": "nest-resolver",
        "*.dto.ts": "nest-controller",
        "*.entity.ts": "database",
        "*.repository.ts": "graphql"
      },
      "material-icon-theme.activeIconPack": "nest",
      "material-icon-theme.folders.associations": {
        "dto": "api",
        "entities": "interface"
      },
      "material-icon-theme.folders.color": "#9b9b9b",
      "liveServer.settings.donotShowInfoMsg": true,
      "liveServer.settings.AdvanceCustomBrowserCmdLine": "" // Cambia el color de las carpetas
}